#This program tests dynamic memory allocation
{
	Set data {} {
		public a;
		public b;

		public Set test {} {
			return a$ + b$;
		}
		public Set setA {i} {
			a = i$;
			return a$;
		}
	}
Begin
	myData = new data;

	myData->data.setA[5]^;
	myData->data.b = 3;

	moreData = new myData$;
	moreData->data.test[];

	free myData$;
	free moreData$;
End
}
