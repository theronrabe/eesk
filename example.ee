#Example Program. Theron Rabe. 2013.
{
	given X
	given Y
	
	#
	#This function doubles the first input and adds it to the second input.
	#
	Function twoXplusY {given A given B} {
		public Function add {given X given Y} {} {
			return (X$ + Y$);
		}
	}{
		return add[2*A$; B$];
	}

Begin
	
	X = 34;
	Y = 3;

	if {(X$ < 10) | (X$ > 20)} {
		twoXplusY[X$ Y$];
	}{
		twoXplusY.add[X$ Y$];
	}
End
}
